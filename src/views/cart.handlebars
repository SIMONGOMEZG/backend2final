<h1>Carrito</h1>
<table>
  <thead>
    <tr>
      <th>Producto</th>
      <th>Precio</th>
      <th>Cantidad</th>
      <th>Subtotal</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <td>{{this.product.title}}</td>
      <td>${{this.product.price}}</td>
      <td>{{this.quantity}}</td>
      <td>${{multiply this.product.price this.quantity}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
